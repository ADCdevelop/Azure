			},
			{
				"name": "Get Data For REPORTING",
				"type": "DatabricksNotebook",
				"dependsOn": [
					{
						"activity": "Run data flow for vaccinations",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": []
			},
			{
				"name": "Copy To BlobStorage From GIT",
				"type": "Copy",
				"dependsOn": [],
