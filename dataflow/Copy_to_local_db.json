{
	"name": "Copy_to_local_db",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CSV_CovidTrackingDataFinal",
						"type": "DatasetReference"
					},
					"name": "CSVsource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DIR_psresults",
						"type": "DatasetReference"
					},
					"name": "CSVdestination"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Country as string,",
				"          ISO3 as string,",
				"          Date as date,",
				"          DailyConfirmedCases as integer,",
				"          DailyDeaths as integer,",
				"          DailyRecovered as integer,",
				"          DailyTests as integer,",
				"          {7DaysCumulatedTests} as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> CSVsource",
				"CSVsource sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'table') ~> CSVdestination"
			]
		}
	}
}